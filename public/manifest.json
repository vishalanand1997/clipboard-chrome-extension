{
  "manifest_version": 2,
  "name": "Text Selection",
  "description": "A sample extension to get the selected text",
  "version": "1.0",
  "browser_action": {
    "default_popup": "index.html"
  },
  "action": {
    "default_icon": {
      "16": "logo192.png",
      "48": "logo192.png",
      "128": "logo192.png"
    },
    "default_title": "Click Me",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "permissions": [
    "activeTab",
    "clipboardWrite"
  ]
}